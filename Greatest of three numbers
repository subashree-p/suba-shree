#include<stdio.h>
int main()
{
	int a,b,c;
	printf("enter three numbers A,B and C : ");
	scanf("%d%d%d",&a,&b,&c);
	if(a>b&&a>c)
	{
		printf("A is greater ");
		}
		else if(b>c)
		{
			printf("B is greater ");
			}
			else
			{
				printf("C is greater ");
				}
			
	return 0;
	}
OUTPUT :
       enter three numbers A,B and C : 10 30 20
         B is greatest
