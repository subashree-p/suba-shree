#include<stdio.h>
int main()
{
	int num;
	printf("enter a number : ");
	scanf("%d",&num);
	if(num%2==0)
	{
		printf("the number %d is not prime",num);
		}
		else if(num%1==0&&num%num==0)
		{
			printf("the number %d is  prime",num);
			} 
		
	
	return 0;
	}
	
OUTPUT:
 Enter a number : 3
  The number 3 is prime 
